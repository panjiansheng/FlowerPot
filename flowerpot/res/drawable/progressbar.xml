<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="10dip" />
			<gradient android:startColor="@color/green_deep" android:centerY="0.75"
				android:endColor="@color/green_deep" android:angle="270" />
		</shape>
	</item>


	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="10dp" />
				<gradient android:startColor="@color/blue"
					android:centerColor="@color/blue" android:centerY="0.75"
					android:endColor="@color/blue" android:angle="270" />
			</shape>
		</clip>
	</item>
</layer-list>     
                  